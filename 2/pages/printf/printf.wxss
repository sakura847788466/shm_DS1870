/* pages/printf/printf.wxss */
page{
  width:100%;
  height:100%;
}
.page{
  background-color:#f8f6f6e7;
  height:100%;
}
.tishi{
  height:20px;
  font-size: 14px;
}
.tishi span{
  display: block;
  color:#ff3266;
  text-align: center;
  background-color: #fff;
  line-height:20px;
}

.title{
    overflow: hidden;
    width: 88%;
    height: 112px;
    margin: 11px auto;
    padding-left: 10%;
    border-radius: 12px;
    position: relative;
    box-shadow: 0 0 1px darkgrey;
    background-color:#fffbe9;
}

/* 遮罩层 */
.link_c{
  height: 271px;
  width:95%;
  position:absolute;
  top:18%;
  left:2.5%;
  background-color: rgba(255, 255, 255, 0.9);
  border-radius: 9.6px;
  border-top-right-radius: 16px;
  
}
.msm_print_val {
    resize: none;
    outline: none;
    background: rgba(255, 255, 255, 0.9);
    display: block;
    width: 80%;
    height: 125px;
    margin: 0 auto;
    padding: 23.6px;
    margin-top:10px;
}
textarea {
    font-size: 16px;
    padding: 0 0.3rem;
    outline: none;
    /* font-size: 0.5rem; */
    border-width: 0px;
    border-style: none;
    border-image: none;
    border: 0.02rem solid darkgoldenrod;
}

button{
  height:37.5px;
  width:80%;
  margin-top:10px; 
  background: orange;
  text-align: center;
  color:#fff;
  line-height: 37.5px;
}

.msm_false {
    position: absolute;
    top: 0;
    right: 0;
    width: 32px;
    height: 32px;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: center;
    border-radius: 50%;
}

.top{
  height:32px;
  line-height:32px;
  text-align: center;
  margin-top:8px;
}
/* --------------------------------- */
.print_space_name{
  margin-top:10px;
}
.print_space_money_msm{
  display:inline-block;
  font-size:14px;
  color:#888888;
  margin-top:3px;
}
.sta{
  margin-top:20px;
  color:#888888;
  font-size:14px;

}
.close{
position:absolute;
top:0;
right:0;
width:32px;
height:32px;
background-size:100%;
background-repeat:no-repeat;
background-position:center;
border-radius:50%;

}
.duanxin{
position:absolute;
top:5px;
left:3px;
width:32px;
height:32px;
background-size:100%;
background-repeat:no-repeat;
background-position:center;
border-radius:50%;

